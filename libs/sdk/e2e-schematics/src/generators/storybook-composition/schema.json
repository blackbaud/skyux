{
  "$schema": "http://json-schema.org/schema",
  "$id": "storybook-composition",
  "type": "object",
  "properties": {
    "projectsJson": {
      "type": "string",
      "description": "JSON array of projects to include"
    },
    "baseUrl": {
      "type": "string",
      "description": "Web address containing published Storybooks."
    }
  },
  "required": ["projectsJson", "baseUrl"]
}
