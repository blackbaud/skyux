{
  "name": "skyux-eslint",
  "version": "0.0.0-PLACEHOLDER",
  "author": "Blackbaud, Inc.",
  "description": "An ESLint plugin for SKY UX projects",
  "keywords": [
    "blackbaud",
    "skyux"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blackbaud/skyux.git"
  },
  "bugs": {
    "url": "https://github.com/blackbaud/skyux/issues"
  },
  "homepage": "https://github.com/blackbaud/skyux#readme",
  "ng-add": {
    "save": "devDependencies"
  },
  "ng-update": {
    "packageGroupName": "skyux-eslint",
    "migrations": "@skyux-sdk/eslint-schematics/migrations.json",
    "packageGroup": {
      "@skyux-sdk/eslint-schematics": "0.0.0-PLACEHOLDER"
    }
  },
  "peerDependencies": {
    "@angular-eslint/bundled-angular-compiler": "^21.1.0",
    "@angular-eslint/eslint-plugin-template": "^21.1.0",
    "@angular-eslint/utils": "^21.1.0",
    "@typescript-eslint/utils": "^8.43.0"
  },
  "dependencies": {
    "@skyux-sdk/eslint-schematics": "0.0.0-PLACEHOLDER",
    "@skyux/manifest": "0.0.0-PLACEHOLDER",
    "tslib": "^2.8.1"
  }
}
