{
  "$schema": "http://json-schema.org/schema",
  "title": "JSON schema for @skyux/manifest documentation.json files",
  "type": "object",
  "properties": {
    "groups": {
      "type": "object",
      "patternProperties": {
        "^.*$": {
          "type": "object",
          "properties": {
            "docsIds": {
              "description": "",
              "type": "array",
              "items": {
                "type": "string"
              },
              "minItems": 1,
              "uniqueItems": true
            },
            "primaryDocsId": {
              "type": "string"
            }
          },
          "additionalProperties": false,
          "nullable": false,
          "required": ["docsIds", "primaryDocsId"]
        }
      }
    }
  },
  "required": ["groups"]
}
