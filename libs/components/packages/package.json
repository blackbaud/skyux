{
  "name": "@skyux/packages",
  "version": "0.0.0-PLACEHOLDER",
  "author": "Blackbaud, Inc.",
  "description": "Handles the `ng update` command for SKY UX component libraries.",
  "keywords": [
    "blackbaud",
    "skyux"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blackbaud/skyux.git"
  },
  "bugs": {
    "url": "https://github.com/blackbaud/skyux/issues"
  },
  "homepage": "https://github.com/blackbaud/skyux#readme",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "default": "./src/index.js"
    },
    "./package.json": {
      "default": "./package.json"
    },
    "./polyfills": {
      "default": "./src/polyfills.js"
    }
  },
  "schematics": "./collection.json",
  "ng-add": {
    "save": "dependencies"
  },
  "ng-update": {
    "migrations": "./src/schematics/migrations/migration-collection.json",
    "packageGroup": {
      "@skyux/packages": "0.0.0-PLACEHOLDER",
