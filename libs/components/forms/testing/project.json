{
  "name": "forms-testing",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/components/forms/testing/src",
  "prefix": "sky",
  "tags": ["testing"],
  "implicitDependencies": [
    "core",
    "core-testing",
    "datetime",
    "forms",
    "help-inline-testing",
    "indicators",
    "indicators-testing",
    "phone-field",
    "popovers-testing",
    "testing",
    "validation"
  ],
  "targets": {
    "build": {
      "command": "echo ' üèóÔ∏è  build forms-testing'",
      "dependsOn": [
        {
          "projects": ["forms"],
          "target": "build"
        }
      ]
    },
    "test": {
      "executor": "@angular-devkit/build-angular:karma",
      "options": {
        "tsConfig": "libs/components/forms/testing/tsconfig.spec.json",
        "karmaConfig": "libs/components/forms/testing/karma.conf.js",
        "codeCoverage": true,
        "codeCoverageExclude": ["**/fixtures/**"],
        "styles": [
          "libs/components/theme/src/lib/styles/sky.scss",
          "libs/components/theme/src/lib/styles/themes/modern/styles.scss"
        ],
        "polyfills": [
          "zone.js",
          "zone.js/testing",
          "libs/components/packages/src/polyfills.js"
        ],
        "inlineStyleLanguage": "scss",
        "stylePreprocessorOptions": {
          "includePaths": ["{workspaceRoot}"]
        }
      },
      "configurations": {
        "ci": {
          "browsers": "ChromeHeadlessNoSandbox",
          "codeCoverage": true,
          "progress": false,
          "sourceMap": true,
          "watch": false
        }
      }
    }
  }
}
