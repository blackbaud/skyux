{
  "extends": "@istanbuljs/nyc-config-typescript",
  "extension": ".ts",
  "include": ["schematics/**/*.ts"],
  "exclude": ["**/*.spec.ts", "**/testing/**"],
  "all": true,
  "check-coverage": true,
  "branches": 100,
  "lines": 100,
  "functions": 100,
  "statements": 100,
  "reporter": ["text", "html", "json"],
  "report-dir": "../../coverage/i18n-schematics",
  "temp-dir": "../../.nyc_output"
}
