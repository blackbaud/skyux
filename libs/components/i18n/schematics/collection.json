{
  "$schema": "../../../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "ng-add": {
      "description": "Add @skyux/i18n to your project",
      "factory": "./ng-add/ng-add.schematic"
    },
    "lib-resources-module": {
      "description": "Generate library resources module",
      "factory": "./ng-generate/lib-resources-module/lib-resources-module.schematic",
      "schema": "./ng-generate/lib-resources-module/schema.json"
    },
    "remote-modules-resources-module": {
      "description": "Generate resources module to be consumed by remote modules.",
      "factory": "./ng-generate/remote-modules-resources-module/remote-modules-resources-module.schematic",
      "schema": "./ng-generate/remote-modules-resources-module/schema.json"
    }
  }
}
