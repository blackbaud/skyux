{
  "name": "indicators-storybook-e2e",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/e2e/indicators-storybook-e2e/src",
  "projectType": "application",
  "tags": [
    "icons-e2e",
    "icon:alert",
    "icon:attach",
    "icon:clipboard-multiple",
    "icon:sparkles",
    "icon:star"
  ],
  "implicitDependencies": ["indicators-storybook"],
  "metadata": {
    "icons-e2e": {
      "icons": ["alert", "attach", "clipboard-multiple", "sparkles", "star"]
    }
  },
  "targets": {
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/e2e/indicators-storybook-e2e/cypress.config.ts",
        "devServerTarget": "indicators-storybook:storybook",
        "baseUrl": "http://localhost:4400",
        "testingType": "e2e"
      },
      "configurations": {
        "ci": {
          "devServerTarget": "indicators-storybook:static-storybook:ci",
          "browser": "chrome",
          "baseUrl": "http://localhost:4200"
        },
        "icons-e2e": {
          "spec": "**/icon.component.cy.ts"
        },
        "icons-e2e-ci": {
          "devServerTarget": "indicators-storybook:storybook:ci",
          "browser": "chrome",
          "spec": "**/icon.component.cy.ts"
        },
        "prebuilt": {
          "devServerTarget": "indicators-storybook:static-storybook:prebuilt",
          "baseUrl": ""
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["{projectRoot}/**/*.{js,ts}"]
      }
    }
  }
}
