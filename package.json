{
  "name": "skyux",
  "version": "5.6.1",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "build": "nx run-many --target=build --all --parallel --maxParallel=5",
    "postbuild": "nx run-many --target=postbuild --all --parallel --maxParallel=5",
    "build:affected": "nx affected:build --parallel --maxParallel=5 && nx affected --target=postbuild --parallel --maxParallel=5",
    "test": "nx run-many --target=test --all --browsers=ChromeHeadless --parallel --maxParallel=5",
    "posttest": "nx run-many --target=posttest --all --parallel --maxParallel=5",
    "test:affected": "nx affected:test --browsers=ChromeHeadless --parallel --maxParallel=5 && nx affected --target=posttest --parallel --maxParallel=5",
    "ci:check-library-dependencies": "ts-node --project ./scripts/tsconfig.json ./scripts/check-library-dependencies.ts",
    "ci:check-library-missing-peers": "ts-node --project ./scripts/tsconfig.json ./scripts/check-library-missing-peers.ts",
    "ci:create-packages-dist": "ts-node --project ./scripts/tsconfig.json ./scripts/create-packages-dist.ts",
    "ci:publish-packages-dist": "ts-node --project ./scripts/tsconfig.json ./scripts/publish-packages-dist.ts",
    "ci:test-affected": "ts-node --project ./scripts/tsconfig.json ./scripts/test-affected.ts",
    "dev:commit": "npx cz",
    "dev:create-release": "ts-node --project ./scripts/tsconfig.json ./scripts/create-release.ts",
    "dev:format": "npx prettier --write .",
    "dev:pristine": "ts-node --project ./scripts/tsconfig.json ./scripts/dev-pristine"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": true,
  "dependencies": {
